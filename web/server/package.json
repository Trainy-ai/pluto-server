{
  "name": "@mlop/server",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "check-types": "tsc --noEmit",
    "lint": "eslint .",
    "lint:prisma": "eslint . -c eslint.config.strict.js",
    "test:setup": "dotenv -e .env.test -- tsx tests/setup.ts",
    "test:setup:cleanup": "dotenv -e .env.test -- tsx tests/setup.ts cleanup",
    "test:smoke": "dotenv -e .env.test -- vitest run --reporter=verbose",
    "test:smoke:coverage": "dotenv -e .env.test -- vitest run --reporter=verbose --coverage",
    "test:smoke:watch": "dotenv -e .env.test -- vitest",
    "test:smoke:local": "dotenv -e .env.locals -- vitest run",
    "test:smoke:dev": "dotenv -e .env.dev -- vitest run",
    "test:cache": "vitest run tests/cache.test.ts --reporter=verbose",
    "seed:dev": "dotenv -e .env.locals -- tsx scripts/seed-dev.ts",
    "seed:dev:docker": "dotenv -e .env -- tsx scripts/seed-dev.ts",
    "dev:local": "pnpm db:generate:local && dotenv -e .env.locals -- next dev -p 3001",
    "dev:dev": "pnpm db:generate:dev && dotenv -e .env.dev -- next dev -p 3001",
    "dev:prod": "pnpm db:generate:prod && dotenv -e .env.prod -- next dev -p 3001",
    "build": "prisma generate && next build",
    "build:local": "pnpm db:generate:local && dotenv -e .env.locals -- next build",
    "build:dev": "pnpm db:generate:dev && dotenv -e .env.dev -- next build",
    "build:prod": "pnpm db:generate:prod && dotenv -e .env.prod -- next build",
    "start:local": "dotenv -e .env.locals -- next start -p 3001",
    "start:dev": "dotenv -e .env.dev -- next start -p 3001",
    "start:prod": "dotenv -e .env.prod -- next start -p 3001",
    "db:generate:local": "dotenv -e .env.locals prisma generate",
    "db:generate:dev": "dotenv -e .env.dev prisma generate",
    "db:generate:prod": "dotenv -e .env.prod prisma generate",
    "db:push:local": "dotenv -e .env.locals prisma db push",
    "db:push:dev": "dotenv -e .env.dev prisma db push",
    "db:push:prod": "dotenv -e .env.prod prisma db push",
    "db:migrate:create:local": "dotenv -e .env.locals prisma migrate dev --create-only",
    "db:migrate:create:dev": "dotenv -e .env.dev prisma migrate dev --create-only",
    "db:migrate:create:prod": "echo 'Not allowed!'",
    "db:migrate:deploy:local": "dotenv -e .env.locals prisma migrate deploy && pnpm db:generate:local",
    "db:migrate:deploy:dev": "dotenv -e .env.dev prisma migrate deploy && pnpm db:generate:dev",
    "db:migrate:deploy:prod": "echo 'Not allowed!'",
    "db:reset:local": "dotenv -e .env.locals prisma migrate reset",
    "db:reset:dev": "dotenv -e .env.dev prisma migrate reset",
    "db:migrate:prod": "dotenv -e .env.prod prisma migrate deploy",
    "studio:local": "dotenv -e .env.locals prisma studio",
    "studio:dev": "dotenv -e .env.dev prisma studio",
    "studio:prod": "dotenv -e .env.prod prisma studio",
    "prebuild": "prisma generate && pnpm check-types"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.787.0",
    "@aws-sdk/s3-request-presigner": "^3.787.0",
    "@clickhouse/client-web": "^1.11.0",
    "@hono/node-server": "^1.14.1",
    "@hono/swagger-ui": "^0.5.3",
    "@hono/trpc-server": "^0.3.4",
    "@hono/zod-openapi": "0.18.3",
    "@hono/zod-validator": "^0.4.3",
    "@prisma/client": "5.2.0",
    "@trpc/server": "11.1.0",
    "@types/nanoid": "^3.0.0",
    "better-auth": "^1.4.10",
    "dotenv-cli": "^8.0.0",
    "drizzle-orm": "^0.41.0",
    "hono": "^4.7.6",
    "nanoid": "^5.1.5",
    "next": "^15.5.7",
    "prisma": "5.2.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "resend": "^6.7.0",
    "sqids": "^0.3.0",
    "superjson": "^2.2.2",
    "zod": "^3.24.2",
    "redis": "^4.7.0",
    "lru-cache": "^10.4.3"
  },
  "devDependencies": {
    "@better-auth/sso": "^1.4.10",
    "@vitest/coverage-v8": "^2.1.8",
    "@noble/hashes": "^2.0.1",
    "@types/bcrypt": "^6.0.0",
    "@types/node": "^22.14.1",
    "@types/react": "19.2.7",
    "@vitest/ui": "^2.1.8",
    "bcrypt": "^6.0.0",
    "drizzle-kit": "^0.30.6",
    "esbuild": "^0.25.2",
    "eslint": "^9.17.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.53.0",
    "vitest": "^2.1.8"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "esbuild",
      "prisma",
      "sharp"
    ]
  }
}
